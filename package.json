{
  "name": "address-book",
  "version": "1.0.0",
  "description": "A sample code project for an interview with an address book in React and some Kotlin command line utilities",
  "main": "index.js",
  "scripts": {
    "install-react-dependencies": "cd react-address-book && npm i",
    "build-site": "cd react-address-book && npm run build",
    "serve-site": "cd react-address-book && npm run serve",
    "build-converter": "cd kotlin-converter && mvn package",
    "convert-xml": "java -jar kotlin-converter/target/kotlin-converter-1.0-SNAPSHOT-jar-with-dependencies.jar ab.xml ab.json",
    "convert-json": "java -jar kotlin-converter/target/kotlin-converter-1.0-SNAPSHOT-jar-with-dependencies.jar ab.json test.xml",
    "build-shuffle": "cd shuffle && mvn package",
    "shuffle": "java -jar shuffle/target/shuffle-1.0-SNAPSHOT-jar-with-dependencies.jar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrevorRanney/address-book.git"
  },
  "keywords": [
    "React",
    "Kotlin",
    "Address",
    "Book"
  ],
  "author": "Trevor Ranney",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/TrevorRanney/address-book/issues"
  },
  "homepage": "https://github.com/TrevorRanney/address-book#readme"
}
